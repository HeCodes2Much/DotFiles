# shellcheck disable=all

lines=$(compgen -c | sort -u | wc -l)
programs=$(compgen -c | sort -u | tail -n +9)
echo -e "$lines
$programs" | fzf --prompt="Run Program: " --border=rounded --margin=1% --color=dark --height 100% --reverse --header="   PROGRAM MENU " --info=hidden --header-first
