#!/usr/bin/env bash

# shellcheck disable=all
terminal='alacritty --title=scratchpad'
files='alacritty --title=clifm -e clifm'
volume='pavucontrol'

case "$1" in
	terminal)
        entry=$terminal
        ;;
	files)
        entry=$files
        ;;
    volume)
        entry=$volume
        ;;
esac

$entry &
